  "type": "library",
  "keywords": [
    "originphp"
  ],
  "homepage": "https://www.originphp.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Jamiel Sharief",
      "email": "js@originphp.com"
    }
  ],
  "autoload": {
